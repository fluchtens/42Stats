generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Campus {
  id      Int    @id
  name    String @db.VarChar(255)
  country String @db.VarChar(255)
}
